{
  "name": "futuremarketingai-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "tsc && vite build --mode analyze",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 700",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css,md}\"",
    "preview": "vite preview",
    "prepare": "husky",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lighthouse": "npm run build && npm run preview & sleep 5 && lighthouse http://localhost:4173 --view --preset=desktop && kill %1",
    "perf": "npm run build && npm run preview",
    "analyze": "npm run build:analyze && open dist/stats.html"
  },
  "dependencies": {
    "@react-three/drei": "9.114.3",
    "@react-three/fiber": "8.17.10",
    "@sentry/react": "^10.18.0",
    "@types/d3": "^7.4.3",
    "country-flag-icons": "^1.5.21",
    "d3": "^7.9.0",
    "framer-motion": "^11.0.0",
    "gsap": "^3.13.0",
    "html2canvas": "^1.4.1",
    "i18next": "^25.5.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.545.0",
    "openai": "^6.2.0",
    "react": "^18.3.1",
    "react-calendly": "^4.4.0",
    "react-cookie-consent": "^9.0.0",
    "react-dom": "^18.3.1",
    "react-ga4": "^2.1.0",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^16.0.0",
    "react-icons": "^5.5.0",
    "react-is": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.3",
    "react-use": "^17.6.0",
    "recharts": "^3.2.1",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1",
    "three": "0.168.0",
    "vite-plugin-sitemap": "^0.8.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@playwright/test": "^1.56.0",
    "@storybook/addon-a11y": "^9.1.10",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-onboarding": "^9.1.10",
    "@storybook/addon-vitest": "^9.1.10",
    "@storybook/react-vite": "^9.1.10",
    "@testing-library/jest-dom": "^6.9.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.7.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "eslint-plugin-storybook": "^9.1.10",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.3",
    "postcss": "^8.4.49",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^6.0.3",
    "storybook": "^9.1.10",
    "tailwindcss": "^3.4.17",
    "terser": "^5.44.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.5",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^3.2.4",
    "web-vitals": "^5.1.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
